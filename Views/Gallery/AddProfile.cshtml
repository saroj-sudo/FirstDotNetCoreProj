@model Profile
@{
    ViewData["Title"] = "AddProfile";
}
<div class="row">
    <div class="col-md-6 offset-3">
        <h1>AddProfile</h1>
        <form asp-action="AddProfile" method="post" enctype="multipart/form-data">
            <label>Name</label>
            <input class="form-control" asp-for="FullName" />
            <label>Choose Image</label>
            <input name="image" type="file" />
            <button class="form-control btn btn-success" type="submit">Submit</button>
        </form>
    </div>
</div>



